<div class="container">
  <h1>All Cities</h1>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Zip Code</th>
      </tr>
    </thead>
    <tbody>
      <% @cities.each do |city| %>
        <tr>
          <td><%= city.name %></td>
          <td><%= city.zip_code %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>